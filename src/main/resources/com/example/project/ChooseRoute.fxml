<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="355.0" prefWidth="255.0" style="-fx-background-color: #f5f5dc;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.ChooseRouteController">
    <children>
        <VBox layoutX="3.0" layoutY="286.0" prefHeight="70.0" prefWidth="250.0">
            <children>
                <HBox layoutX="10.0" layoutY="10.0" prefHeight="70.0" prefWidth="324.0">
                    <children>
                        <Button fx:id="RChoose" mnemonicParsing="false" onMouseClicked="#chooseRoute" prefHeight="40.0" prefWidth="97.0" style="-fx-background-color: #793300; -fx-background-radius: 30PX;" text="Choose" textFill="BEIGE">
                            <HBox.margin>
                                <Insets left="75.0" top="15.0" />
                            </HBox.margin>
                            <font>
                                <Font name="Gill Sans MT Bold" size="17.0" />
                            </font>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
        <TableView fx:id="table" layoutX="6.0" layoutY="8.0" prefHeight="278.0" prefWidth="244.0">
            <columns>
                <TableColumn fx:id="RID" prefWidth="46.0" text="RID" />
                <TableColumn fx:id="Distance" prefWidth="94.0" text="Distance" />
                <TableColumn fx:id="Name" prefWidth="101.0" text="Name" />
            </columns>
        </TableView>
    </children>
</Pane>
